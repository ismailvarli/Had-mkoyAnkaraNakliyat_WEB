@section Scripts {
    <style>
        .pagination {
            list-style: none;
            display: flex;
            gap: .5rem;
            padding-left: 0;
            margin: 0
        }

        .page-item .page-link {
            display: block;
            padding: .5rem .75rem;
            border: 1px solid #ddd;
            border-radius: .375rem;
            text-decoration: none
        }

        .page-item.disabled .page-link {
            pointer-events: none;
            opacity: .5
        }

        .page-item.active .page-link {
            background: #ff7300;
            color: #fff;
            border-color: #ff7300
        }

        .srv-hidden {
            display: none !important;
        }
    </style>

    <script>
        (function () {
          var pageSize = 9; // her sayfada gösterilecek kart sayısı (3x3)
          var grid = document.getElementById('servicesGrid');
          if (!grid) return;

          // sadece ilk seviye çocukları (sizin col-... öğeleri)
          var items = Array.prototype.slice.call(grid.querySelectorAll(':scope > div'));
          var total = items.length;
          var totalPages = Math.max(1, Math.ceil(total / pageSize));

          var params = new URLSearchParams(window.location.search);
          var startPage = parseInt(params.get('page') || '1', 10);
          if (isNaN(startPage) || startPage < 1) startPage = 1;
          if (startPage > totalPages) startPage = totalPages;

          function showPage(p) {
            items.forEach(function (el, idx) {
              var visible = idx >= (p - 1) * pageSize && idx < p * pageSize;
              el.classList.toggle('srv-hidden', !visible);
            });
            buildPager(p);
            params.set('page', p);
            var url = window.location.pathname + '?' + params.toString();
            window.history.replaceState(null, '', url);
          }

          function buildPager(current) {
            var ul = document.getElementById('servicesPager');
            if (!ul) return;
            ul.innerHTML = '';

            function addItem(disabled, active, label, targetPage, aria) {
              var li = document.createElement('li');
              li.className = 'page-item' + (disabled ? ' disabled' : '') + (active ? ' active' : '');
              var a = document.createElement('a');
              a.className = 'page-link';
              a.href = 'javascript:void(0)';
              a.textContent = label;
              if (aria) a.setAttribute('aria-label', aria);
              if (!disabled) a.addEventListener('click', function () { showPage(targetPage); });
              li.appendChild(a);
              ul.appendChild(li);
            }

            addItem(current <= 1, false, '«', current - 1, 'Önceki');

            var maxBtns = 7;
            var start = Math.max(1, current - 3);
            var end = Math.min(totalPages, start + maxBtns - 1);
            if (end - start + 1 < maxBtns) start = Math.max(1, end - maxBtns + 1);

            for (var i = start; i <= end; i++) {
              addItem(false, i === current, String(i), i);
            }

            addItem(current >= totalPages, false, '»', current + 1, 'Sonraki');
          }

          showPage(startPage);
        })();
    </script>
}
