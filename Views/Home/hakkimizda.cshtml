<style>
    .animated-alert {
        padding: 15px 20px;
        margin: 20px 0 30px 0;
        border-radius: 8px;
        font-weight: 500;
        font-size: 16px;
        display: flex;
        align-items: center;
        gap: 10px;
        opacity: 0;
        animation: fadeInSlide 0.6s ease-out forwards;
        box-shadow: 0 4px 16px rgba(0,0,0,0.10);
        border: 1.5px solid #e1e4e8;
    }

        .animated-alert.success {
            background: #e7ffe9;
            color: #106a1d;
            border-color: #7ee195;
        }

        .animated-alert.error {
            background: #fff3f1;
            color: #a32517;
            border-color: #f7c0b5;
        }

        .animated-alert .icon {
            font-size: 22px;
            margin-right: 6px;
        }

    @@keyframes fadeInSlide {
        from

    {
        opacity: 0;
        transform: translateY(-16px);
    }

    to {
        opacity: 1;
        transform: translateY(0);
    }

    }

